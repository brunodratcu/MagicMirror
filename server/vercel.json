{
    "rewrites": [
        { "source": "/(.*)", "destination": "/api/index" }
    ],
    "functions": {
        "server/*.py": {
            "runtime": "python3.11"
        }
    },
    "builds": [
        {
            "src": "server/*.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/app.py"
        }
    ]
}